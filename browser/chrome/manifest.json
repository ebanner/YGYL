{
    "manifest_version": 2,
  
    "name": "YGYL Player",
    "description": "Play all WebMs on the YGYL page.",
    "version": "1.0",
  
    "icons": {
      "48": "icon.png"
    },
  
    "permissions": [
      "tabs", 
      "<all_urls>",
      "https://a.4cdn.org/"
    ],
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    }
  }